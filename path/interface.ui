   <string notr="true">#MainWindow {
background-image: url(:/fons/fons/stock-photo-beautiful-light-blue-sky-clouds.jpg);
}

QPushButton {
background-color: transparent;
border: 2px solid transparent;
border-radius: 5px;
}

QFrame {
border-radius: 5px;
}

#nameCityCountry,#condition,#prognozDn, #prognoz14, #temp {
font-family: &quot;Verdana&quot;;
font-weight: bold;
}

QPushButton:hover {
background-color: rgba(255, 255, 255, 0.5);
border: 2px solid transparent;
}

QPushButton:pressed {
background-color: rgba(255, 255, 255, 0.7);
border: 2px solid transparent
}

#hourly_forecast,  #daily_forecast {
border-radius: 5px;
background-color:  rgba(255, 255, 255, 0.7);
padding: 5px;
}

#hourly_forecast:hover,  #daily_forecast:hover {
border: 2px solid rgba(232, 232, 232, 0.7);
}


/*скролл арея*/
QScrollArea {
background-color: transparent;
border: none;
border-radius: 5px;
}

#daily_scrollAreaCont, #hourly_scrollAreaCont {
background-color: transparent;
}


/*скроллбары*/
QScrollArea QFrame {
background-color: transparent;
}

QScrollBar:horizontal {
background-color: transparent;
height: 8px;
}

QScrollBar {
background-color: transparent;
}

QScrollBar::handle {
background-color: rgba(255, 255, 255, 0.7);
border-radius: 3px;
border: none;
}

QScrollBar::handle:hover, QScrollBar::handle:pressed {
background-color: rgba(255, 255, 255, 0.9);
}

QScrollBar::add-page, QScrollBar::sub-page {
background-color: transparent;
}

QScrollBar::add-line, QScrollBar::sub-line {
background-color: transparent;
width: 0px;
height: 0px;
}

QScrollBar::add-line:horizontal {
border-right: 1px solid #fff;
}

QScrollBar::sub-line:horizontal {
border-left: 1px solid #fff;
}

QScrollBar::handle:horizontal {
min-width: 20px;
}



/*поиск*/
#searhPanel {
background-color: rgba(255, 255, 255, 0.7);
border-radius: 5px;
padding: 5px;
}

#searhPanel:hover {
border: 2px solid rgba(232, 232, 232, 0.7);
}



#searchEdit {
background-color: rgba(255, 255, 255, 0);
border: none;
}</string>
